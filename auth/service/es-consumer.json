{
  "name": "es-consumer",
  "host": "http://elasticsearch-consumer:9099",
  "oidc_endpoints": [
    {
      "name": "protected",
      "paths": [
        "/{realm}/{name}"
      ],
      "strip_path": "true",
      "oidc_override": {
        "config.user_keys": [
          "preferred_username",
          "email",
          "groups"
        ]
      }
    }
  ]
}
