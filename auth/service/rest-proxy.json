{
  "name": "rest-proxy",
  "host": "http://rest-proxy:3333",
  "oidc_endpoints": [
    {
      "name": "protected",
      "paths": [
        "/{realm}/{name}$",
        "/{realm}/{name}/"
      ],
      "strip_path": "true",
      "oidc_override": {
        "config.user_keys": [
          "preferred_username",
          "email",
          "groups"
        ]
      }
    }
  ]
}
